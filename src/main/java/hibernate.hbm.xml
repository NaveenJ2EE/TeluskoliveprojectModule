<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="com.telusko.model.Feedback" table="feedbacktable" catalog="naveen">

<id name="feedbackid" type="java.lang.Integer">
<generator class="increment"></generator>
</id>

<property name="name" type="java.lang.String" >
</property>


<property name="email" type="java.lang.String" >
</property>


<property name="websitemode" type="java.lang.String">

</property>


<property name="code"  type="java.lang.Integer">
</property>

<property name="message"  type="java.lang.String">
</property>

<property name="rating"  type="java.lang.Integer">
</property>





</class>
</hibernate-mapping>